{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.0.0",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-soap": "*",
    "ext-calendar": "*",
    "ext-simplexml": "*",
    "ext-openssl": "^7.0",
    "ext-redis": "*",
    "laravel/framework": "5.5.*",
    "laravel/tinker": "^1.0",
    "league/flysystem-aws-s3-v3": "~1.0",
    "dingo/api": "2.1.*",
    "tymon/jwt-auth": "1.0.*",
    "predis/predis": "1.1.*",
    "facebook/php-ads-sdk": "10.0.*",
    "tinify/tinify": "^1.5",
    "jenssegers/mongodb": "3.3.*",
    "brozot/laravel-fcm": "1.3.*",
    "aws/aws-sdk-php": "3.177.*",
    "sunra/php-simple-html-dom-parser": "1.5.*",
    "google/apiclient": "2.9.*",
    "barryvdh/laravel-dompdf": "0.8.*",
    "folklore/graphql": "1.1.*",
    "intervention/image": "2.5.*",
    "maatwebsite/excel": "2.1.*",
    "league/fractal": "0.17.*",
    "sentry/sentry-laravel": "^0.11.0",
    "algolia/algoliasearch-laravel": "1.7.*",
    "sheba/dal": "dev-feature/laravel-upgrade",
    "sheba/voucher": "dev-master",
    "sheba/notification": "dev-master",
    "sheba/sms": "dev-master",
    "sheba/report": "dev-feature/laravel-upgrade",
    "sheba/elasticsearch": "dev-feature/laravel-upgrade"
  },
  "require-dev": {
    "filp/whoops": "~2.0",
    "fzaninotto/faker": "1.9.*",
    "mockery/mockery": "1.3.*",
    "phpunit/phpunit": "~6.0",
    "symfony/css-selector": "3.2.*",
    "symfony/dom-crawler": "3.2.*",
    "itsgoingd/clockwork": "5.0.*"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "files": [
      "app/Helper/helper_functions.php",
      "app/Sheba/Pap/PapApi.class.php"
    ],
    "psr-4": {
      "App\\": "app/",
      "Sheba\\": "app/Sheba",
      "Tests\\": "tests"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "optimize-autoloader": true
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://shafiq_startern@bitbucket.org/startern/sheba_voucher.git"
    },
    {
      "type": "vcs",
      "url": "https://shafiq_startern@bitbucket.org/startern/sheba_dal.git"
    },
    {
      "type": "vcs",
      "url": "https://shafiq_startern@bitbucket.org/startern/sheba_notification.git"
    },
    {
      "type": "vcs",
      "url": "https://shafiq_startern@bitbucket.org/startern/sheba_sms.git"
    },
    {
      "type": "vcs",
      "url": "https://shafiq_startern@bitbucket.org/startern/sheba_report.git"
    },
    {
      "type": "vcs",
      "url": "https://shafiq_startern@bitbucket.org/startern/sheba_elasticsearch.git"
    }
  ]
}
