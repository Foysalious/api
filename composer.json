{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.5.9",
    "laravel/framework": "5.2.*",
    "league/flysystem-aws-s3-v3": "~1.0",
    "dingo/api": "1.0.*@dev",
    "tymon/jwt-auth": "0.5.*",
    "predis/predis": "^1.1",
    "facebook/php-ads-sdk": "^2.9",
    "tinify/tinify": "^1.5",
    "jenssegers/mongodb": "^3.2",
    "brozot/laravel-fcm": "^1.2",
    "aws/aws-sdk-php": "^3.32",
    "sunra/php-simple-html-dom-parser": "1.5.2",
    "google/apiclient": "^2.2",
    "barryvdh/laravel-dompdf": "0.8.*",
    "folklore/graphql": "^1.0",
    "sheba/dal": "dev-master",
    "sheba/voucher": "dev-master",
    "sheba/notification": "dev-master",
    "sheba/sms": "dev-master",
    "sheba/report": "dev-master",
    "intervention/image": "^2.4",
    "ext-json": "*",
    "ext-curl": "*",
    "ext-soap": "*",
    "ext-calendar": "*",
    "ext-simplexml": "*",
    "maatwebsite/excel": "~2.1.0",
    "league/fractal": "^0.17.0",
    "sentry/sentry-laravel": "^0.11.0",
    "ext-openssl": "^7.0",
    "ext-mcrypt": "^7.0",
    "ext-redis": "*",
    "ext-mysql_xdevapi": "*",
    "ext-intl": "*",
    "algolia/algoliasearch-laravel": "^1.7",
    "firebase/php-jwt": "^5.2",
    "lcobucci/jwt": "^3.3"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~4.0",
    "symfony/css-selector": "2.8.*|3.0.*",
    "symfony/dom-crawler": "2.8.*|3.0.*",
    "itsgoingd/clockwork": "^4.0"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "files": [
      "app/Helper/helper_functions.php",
      "app/Sheba/Pap/PapApi.class.php"
    ],
    "psr-4": {
      "App\\": "app/",
      "Sheba\\": "app/Sheba"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan optimize"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://mac_pasha@bitbucket.org/startern/sheba_voucher.git"
    },
    {
      "type": "vcs",
      "url": "https://shafiq_startern@bitbucket.org/startern/sheba_dal.git"
    },
    {
      "type": "vcs",
      "url": "https://mac_pasha@bitbucket.org/startern/sheba_notification.git"
    },
    {
      "type": "vcs",
      "url": "https://mac_pasha@bitbucket.org/startern/sheba_sms.git"
    },
    {
      "type": "vcs",
      "url": "https://mac_pasha@bitbucket.org/startern/sheba_report.git"
    }
  ]
}
